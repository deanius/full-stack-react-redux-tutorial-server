{
  "name": "react-trivia-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predeps": "rm -rf tmp/deps; mkdir -p tmp/deps/client/src; mkdir tmp/deps/src; echo",
    "deps": "npm run deps:server; npm run deps:client",
    "deps:server": "babel -d tmp/deps/src src/; madge tmp/deps/src/* -i deps-server.png; open deps-server.png",
    "deps:client": "babel -d tmp/deps/client/src client/src; madge tmp/deps/client/src -x TestHarness -i deps-client.png; open deps-client.png",
    "start": "babel-node src/index.js",
    "client": "webpack-dev-server --host 0.0.0.0 --port 3120",
    "test": "echo 'Test soon'",
    "test:watch": "npm run test -- --watch",
    "deploy": "webpack && git add client/ && git commit -m 'rebundle' && git push heroku master",
    "repl": "node -r reify/repl --use_strict"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-preset-react": "^6.5.0",
    "chai": "3.5.0",
    "css-loader": "^0.23.1",
    "mocha": "2.4.5",
    "postcss-loader": "^0.9.1",
    "reify": "^0.2.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "3.3.1",
    "redux-act": "^0.4.1",
    "socket.io": "1.4.5",
    "uuid": "^2.0.2"
  },
